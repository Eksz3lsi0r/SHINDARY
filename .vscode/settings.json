{
	"github.copilot.nextEditSuggestions.fixes": true,
	"github.copilot.nextEditSuggestions.enabled": true,
	"github.copilot.chat.useCodebase": true,
	"github.copilot.chat.experimental.codebaseContext": true,
	"github.copilot.chat.experimental.contextualSearch": true,
	"github.copilot.chat.experimental.useSymbols": true,
	"github.copilot.chat.experimental.smartSelection": true,
	"github.copilot.chat.experimental.enhancedContext": true,
	"github.copilot.chat.experimental.codeAnalysis": true,
	"github.copilot.chat.experimental.workspaceSearch": true,
	"github.copilot.chat.welcomeMessage": "Hallo! Ich bin GitHub Copilot, bereit f√ºr dein Roblox Lua Subway Surfers Projekt.",
	"github.copilot.chat.localeOverride": "de",
	"github.copilot.advanced": {
		"debug.overrideEngine": "copilot-chat",
		"debug.testOverrideProxyUrl": "",
		"length": 3600,
		"temperature": "0.1",
		"top_p": 1,
		"inlineSuggestCount": 3,
		"listCount": 10,
		"debug.showScores": true
	},
	"github.copilot.chat.enableTerminalContext": true,
	"github.copilot.chat.enableTerminalCodeActions": true,
	"github.copilot.chat.terminal.enabled": true,
	"github.copilot.chat.terminal.shellIntegration": true,
	"editor.inlineSuggest.enabled": true,
	"editor.inlineSuggest.showToolbar": "always",
	"editor.suggestSelection": "first",
	"editor.quickSuggestions": {
		"other": "on",
		"comments": "on",
		"strings": "on"
	},
	"editor.quickSuggestionsDelay": 300,
	"editor.acceptSuggestionOnEnter": "on",
	"editor.tabCompletion": "on",
	"editor.parameterHints.enabled": true,
	"editor.suggest.shareSuggestSelections": false,
	"editor.suggest.showStatusBar": true,
	"editor.bracketPairColorization.enabled": true,
	"editor.autoClosingBrackets": "always",
	"editor.autoClosingQuotes": "always",
	"files.autoSave": "onFocusChange",
	"files.trimTrailingWhitespace": true,
	"files.insertFinalNewline": true,
	"files.eol": "\n",
	"workbench.startupEditor": "none",
	"editor.maxTokenizationLineLength": 50000,
	"editor.minimap.enabled": true,
	"editor.occurrencesHighlight": "off",
	"editor.selectionHighlight": false,
	"editor.defaultFormatter": "JohnnyMorganz.luau-lsp",
	"editor.defaultFoldingRangeProvider": "GitHub.copilot-chat",
	"editor.allowVariableFontsInAccessibilityMode": true,
	"editor.semanticHighlighting.enabled": true,
	"github.copilot.chat.languageContext.fix.typescript.enabled": true,
	"github.copilot.chat.languageContext.inline.typescript.enabled": true,
	"github.copilot.chat.languageContext.typescript.enabled": true,
	"luau-lsp.require.mode": "relativeToFile",
	"luau-lsp.require.directoryAliases": {
		"@shared": "src/shared",
		"@server": "src/server",
		"@client": "src/client"
	},
	"luau-lsp.sourcemap.enabled": true,
	"luau-lsp.sourcemap.autogenerate": true,
	"luau-lsp.sourcemap.rojoProjectFile": "default.project.json",
	"luau-lsp.types.roblox": true,
	"luau-lsp.types.robloxSecurityLevel": "None",
	"luau-lsp.completion.enabled": true,
	"luau-lsp.completion.imports.enabled": true,
	"luau-lsp.inlayHints.parameterNames": true,
	"luau-lsp.inlayHints.typeHints": true,
	"luau-lsp.diagnostics.strict": true,
	"robloxLsp.diagnostics.workspace": true,
	"robloxLsp.misc.parameterHints": true,
	"files.associations": {
		"*.lua": "luau",
		"*.luau": "luau"
	},
	"luau-lsp.platform.type": "roblox",
	"search.useGlobalIgnoreFiles": true,
	"search.useIgnoreFiles": true,
	"search.smartCase": true,
	"search.seedOnFocus": true,
	"search.seedWithNearestWord": true,
	"workbench.search.defaultQuery": {
		"path": "src/"
	},
	"workbench.search.collapseResults": "auto",
	"typescript.suggest.includeCompletionsForModuleExports": true,
	"typescript.suggest.includeCompletionsForImportStatements": true,
	"extensions.ignoreRecommendations": false,
	"workbench.enableExperiments": true,
	"workbench.experimental.copilotChat.enabledInCodeActions": true,
	"workbench.experimental.copilotChat.enabledInInlineChat": true,
	"luau-lsp.completion.imports.suggestRequires": true,
	"luau-lsp.completion.fillCallArguments": true,
	"luau-lsp.diagnostics.enabled": true,
	"luau-lsp.diagnostics.workspace": true,
	"[lua]": {
		"editor.defaultFormatter": "JohnnyMorganz.stylua",
		"editor.formatOnSave": true,
		"editor.formatOnPaste": true,
		"editor.formatOnType": true,
		"editor.codeActionsOnSave": {
			"source.fixAll": "explicit"
		}
	},
	"[luau]": {
		"editor.defaultFormatter": "JohnnyMorganz.stylua",
		"editor.formatOnSave": true,
		"editor.formatOnPaste": true,
		"editor.formatOnType": true,
		"editor.codeActionsOnSave": {
			"source.fixAll": "explicit"
		}
	},
	"files.associations": {
		"*.lua": "luau",
		"*.luau": "luau",
		"*.server.lua": "luau",
		"*.client.lua": "luau",
		"*.module.lua": "luau"
	},
	"files.exclude": {
		"**/node_modules": true,
		"**/Packages": true,
		"**/.git": true,
		"**/.DS_Store": true
	},
	"search.exclude": {
		"**/node_modules": true,
		"**/Packages": true,
		"**/*.rbxl": true,
		"**/*.rbxlx": true
	},
	"github.copilot.enable": {
		"*": true,
		"lua": true,
		"luau": true
	},
	"github.copilot.editor.enableAutoCompletions": true,
	"editor.suggest.localityBonus": true,
	"editor.suggest.snippetsPreventQuickSuggestions": false,
	"editor.wordBasedSuggestions": "matchingDocuments",
	"debug.allowBreakpointsEverywhere": true,
	"debug.openDebug": "openOnDebugBreak",
	"terminal.integrated.defaultProfile.osx": "zsh",
	"terminal.integrated.profiles.osx": {
		"rojo": {
			"path": "/usr/local/bin/rojo",
			"args": [
				"serve"
			]
		}
	},
	"terminal.integrated.persistentSessionReviveProcess": "always",
	"terminal.integrated.enablePersistentSessions": true,
	"terminal.integrated.copyOnSelection": true,
	"terminal.integrated.rightClickBehavior": "default",
	"terminal.integrated.allowMnemonics": true,
	"terminal.integrated.enableVisualBell": false,
	"terminal.integrated.scrollback": 10000,
	"terminal.integrated.fastScrollSensitivity": 5,
	"terminal.integrated.mouseWheelScrollSensitivity": 1,
	"terminal.integrated.sendKeybindingsToShell": true,
	"terminal.integrated.allowChords": true,
	"terminal.integrated.commandsToSkipShell": [],
	"terminal.integrated.shellIntegration.enabled": true,
	"terminal.integrated.shellIntegration.showWelcome": false,
	"terminal.integrated.shellIntegration.decorationsEnabled": "both",
	"terminal.integrated.smoothScrolling": true,
	"explorer.openEditors.visible": 0,
	"explorer.excludeGitIgnore": false,
	"explorer.autoReveal": true,
	"files.watcherExclude": {
		"**/node_modules/**": true,
		"**/.git/**": true,
		"**/Packages/**": true
	},
	"stylua.searchParentDirectories": true,
	"stylua.verify": true,
	"stylua.configPath": "./stylua.toml",
	"rojo.enableAutoBuild": true,
	"rojo.autoConnect": true,
	"rojo.projectFile": "default.project.json",
	"rojo.buildOnSave": true,
	"rojo.syncOnSave": true,
	"rojo.buildOutputDirectory": "build",
	"rojo.enableDiagnostics": true,
	"rojo.showNotifications": true,
	"robloxLsp.enable": true,
	"robloxLsp.diagnostics.enable": true,
	"robloxLsp.completion.enable": true,
	"robloxLsp.hover.enable": true,
	"robloxLsp.signatureHelp.enable": true,
	"robloxLsp.misc.parameters": [
		"--logpath=.vscode/roblox-lsp.log",
		"--loglevel=information"
	],
	"robloxLsp.workspace.library": [
		"src"
	],
	"robloxLsp.workspace.checkThirdParty": false,
	"[markdown]": {
		"editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
	}
}
