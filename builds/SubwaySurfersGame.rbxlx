    platformWidth = 20,
    platformLength = 50,
    platformHeight = 2,
    despawnDistance = 100, -- units behind player
    spawnDistance = 200, -- units ahead of player
}

-- Score values
GameConfig.Scores = {
    coin = 10,
    gem = 50,
    powerUp = 25,
    distanceMultiplier = 1, -- points per stud traveled
}

-- Power-up durations (in seconds)
GameConfig.PowerUps = {
    -- Legacy power-ups
    speedBoost = 5,
    shield = 8,
    magnet = 6,
